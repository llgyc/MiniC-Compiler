int n;
void toh(int no, int src, int dst, int spare) {
	if (no == 1) {
		putch(src);
		putch(dst);
		putch(10);
		return;
	}
	toh(no-1,src, spare, dst);
        putch(src);
        putch(dst);
        putch(10);
	toh(no-1,spare,dst,src);
}
int main() {
	n = getint();
	toh(n, 65, 66, 67);
	return 0;
}
	
